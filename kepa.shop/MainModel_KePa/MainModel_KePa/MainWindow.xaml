<Window x:Class="MainModel_KePa.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MainModel_KePa"
        mc:Ignorable="d"
        Title="MainWindow" Height="1080" Width="1900">
    <Grid>
        <DockPanel>
            <DockPanel.Background>
                <ImageBrush ImageSource="Img/fondo-final.jpg"/>
            </DockPanel.Background>
            <DockPanel Width="660" Height="766" HorizontalAlignment="Left" Margin="80,0,0,0" LastChildFill="False">
                <DockPanel.Background>
                    <ImageBrush ImageSource="Img/portada_giraffes.png"/>
                </DockPanel.Background>
                <Button Content="Button" Height="50" RenderTransformOrigin="0.5,0.5" DockPanel.Dock="Bottom">
                    <Button.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleX="-1"/>
                            <SkewTransform/>
                            <RotateTransform Angle="-1.158"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Button.RenderTransform>
                </Button>

            </DockPanel>

        </DockPanel>
    </Grid>
</Window>
